# Personal account
Host personal 
  HostName github.com
  IdentitiesOnly yes
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_ed25519

# Company account
Host startingout 
  HostName github.com
  IdentitiesOnly yes
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_ed25519_sa

# Default
Host *
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_ed25519
